# https://leetcode.com/contest/weekly-contest-370/problems/find-champion-i/

class Solution:
    def findChampion(self, grid: List[List[int]]) -> int:
        m = len(grid)
        n = len(grid[0])
        check = 0
        for i in range(m):
            sum_i = 0
            for j in range(n):
                    sum_i+=grid[i][j]
            if sum_i==m-1:
                return i